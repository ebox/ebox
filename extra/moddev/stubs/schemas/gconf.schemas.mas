<%args>
    $module
    $mainClass
</%args>

<gconfschemafile>
    <schemalist>
    <schema>
        <key>/schemas/ebox/modules/global/modules/<% $module %>/class</key>
        <applyto>/ebox/modules/global/modules/<% $module %>/class</applyto>
        <owner>ebox</owner>
        <type>string</type>
        <default><% $mainClass %></default>
        <locale name="C"/>
    </schema>
    </schemalist>
    </gconfschemafile>
