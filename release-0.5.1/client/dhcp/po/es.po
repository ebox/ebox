#: src/EBox/DHCP.pm:190 src/EBox/DHCP.pm:297
#, perl-brace-format
msgid "{iface} is not static"
msgstr "{iface} no es estática"

#: src/EBox/DHCP.pm:194
msgid "\"From\" IP address"
msgstr "Dirección IP \"Desde\""

#: src/EBox/DHCP.pm:195
msgid "\"To\" IP address"
msgstr "Dirección IP \"Hasta\""

#: src/EBox/DHCP.pm:200
#, perl-brace-format
msgid "{from}-{to} is an invalid range"
msgstr "{from}-{to} es un rango inválido"

#: src/EBox/DHCP.pm:209
#, perl-brace-format
msgid "Range {from}-{to} is not in network {net}"
msgstr "El rango {from}-{to} no está en la red {net}"

#: src/EBox/DHCP.pm:217
#, perl-brace-format
msgid "Range {new_from}-{new_to} includes interface IP address: {iface_ip}"
msgstr ""
"El rango {new_from}-{new_to} incluye la dirección IP de la interfaz: "
"{iface_ip}"

#: src/EBox/DHCP.pm:228
#, perl-brace-format
msgid ""
"Range {new_from}-{new_to} overlaps with range '{range}': {old_from}-{old_to}"
msgstr ""
"El rango {new_from}-{new_to} coincide con el rango '{range}': {old_from}-"
"{old_to}"

#: src/EBox/DHCP.pm:242
#, perl-brace-format
msgid "Range {new_from}-{new_to} includes fixed address '{name}': {fixed_ip}"
msgstr ""
"El rango {new_from}-{new_to} incluye la asignación estática '{name}': "
"{fixed_ip}"

#: src/EBox/DHCP.pm:312
#, perl-brace-format
msgid "IP address {ip} is not in network {net}"
msgstr "La dirección IP {ip} no está en la red {net}"

#: src/EBox/DHCP.pm:320
#, perl-brace-format
msgid "The selected IP is the interface IP address: {iface_ip}"
msgstr "La IP seleccionada es la dirección IP de la interfaz: {iface_ip}"

#: src/EBox/DHCP.pm:330
#, perl-brace-format
msgid "IP address {ip} is in range '{range}': {old_from}-{old_to}"
msgstr "La dirección {ip} está en el rango '{range}': {old_from}-{old_to}"

#: src/EBox/DHCP.pm:348
#, perl-brace-format
msgid "IP address {ip} is already added as fixed address '{name}'"
msgstr ""
"La dirección IP {ip} ya se ha añadido como la asignación estática '{name}'"

#: src/EBox/DHCP.pm:357
#, perl-brace-format
msgid ""
"MAC address {mac} is already added as fixed address '{name}' for interface "
"'{iface}'"
msgstr ""
"La dirección MAC {mac} ya se ha añadido como la asignación estática '{name}' "
"para la interfaz '{iface}'"

#: src/EBox/DHCP.pm:454
#, perl-brace-format
msgid ""
"The IP address of the virtual interface you're trying to add is already used "
"by the DHCP range '{range}' in the interface '{iface}'. Please, remove it "
"before trying to add a virtual interface using it."
msgstr ""
"La dirección IP de la interfaz virtual que está intentando añadir está "
"siendo usada por el rango DHCP '{range}' en la interfaz '{iface}'. Por "
"favor, elimine el rango antes de intentar añadir una interfaz virtual que "
"utilice esa IP."

#: src/EBox/DHCP.pm:468
#, perl-brace-format
msgid ""
"The IP address of the virtual interface you're trying to add is already used "
"by the DHCP fixed address '{fixed}' in the interface '{iface}'. Please, "
"remove it before trying to add a virtual interface using it."
msgstr ""
"La dirección IP de la interfaz virtual que está intentando añadir está "
"siendo usada por la asignación estática de DHCP '{fixed}' en la interfaz "
"'{iface}'. Por favor, elimine la asignación estática antes de intentar "
"añadir una interfaz virtual que utilice esa IP."

#: src/templates/index.mas:13
msgid "Enable the DHCP server"
msgstr "Activar el servidor DHCP"

#: src/templates/index.mas:22
msgid "DHCP server requires a static interface, configure one first"
msgstr "El servidor DHCP necesita una interfaz estática, configurela primero"

