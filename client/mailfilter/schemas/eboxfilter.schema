attributetype ( 1.3.6.1.4.1.7165.2.1.80 NAME 'subjectmod'
	DESC 'Enable subject rewriting on positive message'
	EQUALITY booleanMatch
	SYNTAX 1.3.6.1.4.1.1466.115.121.1.7 SINGLE-VALUE )
			
attributetype ( 1.3.6.1.4.1.7165.2.1.81 NAME 'hitspolicy'
	DESC 'Needed hits by a message to take a policy in action'
	EQUALITY caseIgnoreIA5Match
	SUBSTR caseIgnoreIA5SubstringsMatch
	SYNTAX 1.3.6.1.4.1.1466.115.121.1.26{256} SINGLE-VALUE )
			
attributetype ( 1.3.6.1.4.1.7165.2.1.82 NAME 'viruslist'
	DESC 'List of accounts to bypass the virus check'
   EQUALITY caseIgnoreIA5Match
   SUBSTR caseIgnoreIA5SubstringsMatch
	SYNTAX 1.3.6.1.4.1.1466.115.121.1.26{256} SINGLE-VALUE )
			
attributetype ( 1.3.6.1.4.1.7165.2.1.83 NAME 'spamlist'
	DESC 'List of accounts to bypass the spam check'
	EQUALITY caseIgnoreIA5Match
	SUBSTR caseIgnoreIA5SubstringsMatch
	SYNTAX 1.3.6.1.4.1.1466.115.121.1.26{256} SINGLE-VALUE )

attributetype ( 1.3.6.1.4.1.7165.2.1.84 NAME 'bheadlist'
	DESC 'List of accounts to bypass the banned headers check'
	EQUALITY caseIgnoreIA5Match
	SUBSTR caseIgnoreIA5SubstringsMatch
	SYNTAX 1.3.6.1.4.1.1466.115.121.1.26{256} SINGLE-VALUE )
						
attributetype ( 1.3.6.1.4.1.7165.2.1.85 NAME 'bannedlist'
	DESC 'List of accounts to bypass the banned extensions and types check'
	EQUALITY caseIgnoreIA5Match
	SUBSTR caseIgnoreIA5SubstringsMatch
	SYNTAX 1.3.6.1.4.1.1466.115.121.1.26{256} SINGLE-VALUE )
			
attributetype ( 1.3.6.1.4.1.7165.2.1.86 NAME 'whitelist'
	DESC 'Whitelist'
	EQUALITY caseIgnoreIA5Match
	SUBSTR caseIgnoreIA5SubstringsMatch
	SYNTAX 1.3.6.1.4.1.1466.115.121.1.26{256} SINGLE-VALUE )
			
attributetype ( 1.3.6.1.4.1.7165.2.1.87 NAME 'blacklist'
	DESC 'Blacklist'
	EQUALITY caseIgnoreIA5Match
	SUBSTR caseIgnoreIA5SubstringsMatch
	SYNTAX 1.3.6.1.4.1.1466.115.121.1.26{256} SINGLE-VALUE )
			
objectclass ( 1.3.6.1.4.1.7165.2.2.33 NAME 'vdmailfilter' AUXILIARY
	DESC 'Filtering properties for a virtual domain'
	SUP top
	MUST ( subjectmod $ hitspolicy ) 
	MAY ( viruslist $ spamlist $ bheadlist $ bannedlist
	      $ whitelist $ blacklist ) )
