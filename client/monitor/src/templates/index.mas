<%doc>
  Show all the monitored graphs in a page
</%doc>
<%args>
  $graphs
</%args>
<%init>
 use Data::Dumper;
</%init>
<script type="text/javascript" src="/data/js/flotr.js">
<script type="text/javascript" src="/data/js/excanvas.js">
% foreach my $graph (@{$graphs}) {
<& /graph.mas,
   id => $graph->{id},
   type => $graph->{type},
   series => $graph->{series}
  &>
% }
