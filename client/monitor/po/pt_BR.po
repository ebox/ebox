#, perl-brace-format
msgid "That is above the {severity} threshold of {bound}."
msgstr "Isso está acima da {severity} threshold do {bound}."

#: src/EBox/Event/Watcher/Monitor.pm:318
#, perl-brace-format
msgid "That is below the {severity} threshold of {bound}."
msgstr "Isso está abaixo da {severity} threshold do {bound}."

