#!/bin/bash

#DEBHELPER#

case "$1" in
    configure)
    kill `pidof gconfd-2` >/dev/null 2>&1 || true

    # Restart eBox apache
    invoke-rc.d ebox apache restart || true

    # Migrate data if needed
    /usr/share/ebox/ebox-migrate /usr/share/ebox-l7-protocols/migration/

    ;;
esac



exit 0
