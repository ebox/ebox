		&& exit 0; \
	      exit 1;; \
	  esac; \
	done; \
	echo ' cd $(top_srcdir) && $(AUTOMAKE) --foreign  po/Makefile'; \
	cd $(top_srcdir) && \
	  $(AUTOMAKE) --foreign  po/Makefile
