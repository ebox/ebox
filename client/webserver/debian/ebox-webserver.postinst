#!/bin/bash

#DEBHELPER#

case "$1" in
	configure)

		kill `pidof gconfd-2` >/dev/null 2>&1 || true

		# Migrate data if needed
		/usr/share/ebox/ebox-migrate /usr/share/ebox-webserver/migration/

		invoke-rc.d ebox apache restart || true
        
		invoke-rc.d ebox webserver restart || true
esac



exit 0
