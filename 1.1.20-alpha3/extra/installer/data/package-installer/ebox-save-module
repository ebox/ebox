#!/usr/bin/perl

use EBox;
use EBox::Global;
use Error qw(:try);

my $modName = $ARGV[0];

die "No module name given" unless ($modName);

EBox::init();

my $global = EBox::Global->getInstance();

die "Module not present" unless ($global->modExists($modName));

my $module = $global->modInstance($modName);
$module->save();
