msgid "Bundle Upload"
msgstr "Pacote Enviado"

#: src/EBox/CGI/UploadBundle.pm:77
msgid "The bundle file has been uploaded successfully"
msgstr "O arquivo do pacote foi enviado com sucesso"

#: src/EBox/SOAP/Global.pm:147
#, perl-brace-format
msgid "{method} is not a public method"
msgstr "{method} não é um método público"

#: src/EBox/SOAP/Global.pm:175
#, perl-brace-format
msgid "{method} is not a defined method"
msgstr "{method} não é um método definido"

