<!-- vim: ts=4 sw=4 nowrap
-->

<gconfschemafile>
	<schemalist>
		<schema>
			<key>/schemas/ebox/global/modules/firewall/class</key>
			<applyto>/ebox/global/modules/firewall/class</applyto>
			<owner>ebox</owner>
			<type>string</type>
			<default>EBox::Firewall</default>
			<locale name="C"/>
		</schema>
		<schema>
			<key>/schemas/ebox/global/modules/firewall/depends</key>
			<applyto>/ebox/global/modules/firewall/depends</applyto>
			<owner>ebox</owner>
			<type>list</type>
			<list_type>string</list_type>
			<default>[objects,network]</default>
			<locale name="C"/>
		</schema>
		<schema>
			<key>/schemas/ebox/global/modules/firewall/menu/fw/text</key>
			<applyto>/ebox/global/modules/firewall/menu/fw/text</applyto>
			<owner>ebox</owner>
			<type>string</type>
			<locale name="C">
				<default>Firewall</default>
			</locale>
		</schema>
		<schema>
			<key>/schemas/ebox/global/modules/firewall/menu/fw/location</key>
			<applyto>/ebox/global/modules/firewall/menu/fw/location</applyto>
			<owner>ebox</owner>
			<type>string</type>
			<default>Firewall/Index</default>
			<locale name="C"/>
		</schema>

		<schema>
			<key>/schemas/ebox/firewall/deny</key>
			<applyto>/ebox/firewall/deny</applyto>
