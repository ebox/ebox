		<schema>
			<key>/schemas/ebox/firewall/services/ssh/port</key>
			<applyto>/ebox/firewall/services/ssh/port</applyto>
			<owner>ebox</owner>
			<type>int</type>
			<default>22</default>
			<locale name="C"/>
		</schema>
		<schema>
			<key>/schemas/ebox/firewall/services/ssh/protocol</key>
			<applyto>/ebox/firewall/services/ssh/protocol</applyto>
			<owner>ebox</owner>
			<type>string</type>
			<default>tcp</default>
			<locale name="C"/>
		</schema>
		<schema>
			<key>/schemas/ebox/firewall/services/ssh/name</key>
			<applyto>/ebox/firewall/services/ssh/name</applyto>
			<owner>ebox</owner>
			<type>string</type>
			<default>ssh</default>
			<locale name="C"/>
		</schema>

		<schema>
			<key>/schemas/ebox/firewall/objects/_global/name</key>
			<applyto>/ebox/firewall/objects/_global/name</applyto>
			<owner>ebox</owner>
			<type>string</type>
			<default>_global</default>
			<locale name="C"/>
		</schema>
		<schema>
			<key>/schemas/ebox/firewall/objects/_global/policy</key>
			<applyto>/ebox/firewall/objects/_global/policy</applyto>
			<owner>ebox</owner>
			<type>string</type>
			<default>deny</default>
			<locale name="C"/>
		</schema>
		<schema>
			<key>/schemas/ebox/firewall/objects/_global/services/administration/name</key>
			<applyto>/ebox/firewall/objects/_global/services/administration/name</applyto>
			<owner>ebox</owner>
			<type>string</type>
			<default>administration</default>
			<locale name="C"/>
		</schema>
		<schema>
			<key>/schemas/ebox/firewall/objects/_global/services/administration/policy</key>
			<applyto>/ebox/firewall/objects/_global/services/administration/policy</applyto>
			<owner>ebox</owner>
			<type>string</type>
			<default>allow</default>
			<locale name="C"/>
		</schema>

