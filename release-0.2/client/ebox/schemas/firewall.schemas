			<type>int</type>
			<default>80</default>
			<locale name="C"/>
		</schema>
		<schema>
			<key>/schemas/ebox/firewall/services/administration/protocol</key>
			<applyto>/ebox/firewall/services/administration/protocol</applyto>
			<owner>ebox</owner>
			<type>string</type>
			<default>tcp</default>
			<locale name="C"/>
		</schema>
		<schema>
			<key>/schemas/ebox/firewall/services/administration/name</key>
			<applyto>/ebox/firewall/services/administration/name</applyto>
			<owner>ebox</owner>
			<type>string</type>
			<default>administration</default>
			<locale name="C"/>
