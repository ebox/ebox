<!-- vim: ts=4 sw=4
-->
	<TMPL_IF NAME="unsaved">
	<div class='help'>
		<TMPL_VAR EXPR="__('Help about save or discard configuration changes, for all modules!!!')">
	</div>
	<form action='Finish' method='POST'>
		<p class='endform'>
			<input class='inputButton' type='submit' name='save'
				   value='<TMPL_VAR EXPR="__('Save')">' />
			<input class='inputButton' type='submit' name='cancel'
				   value='<TMPL_VAR EXPR="__('Discard changes')">' />
		</p>
	</form>
	<TMPL_ELSE>
	<TMPL_VAR EXPR="__('There are no unsaved changes.')">
	</TMPL_IF>
