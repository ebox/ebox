	<TMPL_IF NAME="unsaved">
	<div class='error'>
		<TMPL_VAR EXPR="__('There are unsaved changes, if you log out now they will be lost.')">
	</div>
	<br />
	<a class='logout' href='/ebox/Logout/Logout'>&raquo; <TMPL_VAR EXPR="__('Exit anyway')"></a>
	<TMPL_ELSE>
	<a class='logout' href='/ebox/Logout/Logout'>&raquo; <TMPL_VAR EXPR="__('Exit')"></a>
	</TMPL_IF>
