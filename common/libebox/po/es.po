#: debian/libebox/usr/share/perl5/EBox/FileSystem.pm:176
#: debian/libebox/usr/share/perl5/EBox/FileSystem.pm:263
#: src/EBox/FileSystem.pm:176 src/EBox/FileSystem.pm:263
#, perl-brace-format
msgid "Directory not found: {d}"
msgstr "Directorio no encontrado: {d}"

#: debian/libebox/usr/share/perl5/EBox/Config.pm:75 src/EBox/Config.pm.in:75
#, perl-brace-format
msgid "Could not open the config file {file}."
msgstr "No se pudo abrir el fichero de configuración {file}"

#: debian/libebox/usr/share/perl5/EBox/Config.pm:169 src/EBox/Config.pm.in:169
msgid "The ebox user has not been set in the config file."
msgstr ""
"El usuario de ebox no ha sido establecido en el fichero de configuración."

