#: src/EBox/Exceptions/DataNotFound.pm:30
#, perl-brace-format
msgid "{data} {value} does not exist."
msgstr "{data} no existente: {value}."

#: src/EBox/Exceptions/DataExists.pm:31
#, perl-brace-format
msgid "{data} {value} already exists."
msgstr "{data} ya existente: {value}."

#: src/EBox/Exceptions/InvalidData.pm:31
#, perl-brace-format
msgid "Invalid value for {data}: {value}."
msgstr "{data} inválido, valor: {value}."

#: src/EBox/Exceptions/DataMissing.pm:30
#, perl-brace-format
msgid "{data} is empty."
msgstr "Dato vacío: {data}"

#: src/EBox/CGI/Run.pm:58
msgid "The page could not be found"
msgstr "La página no pudo encontrarse"

#: src/EBox/CGI/Base.pm:169
msgid ""
"The input contains invalid characters. All alphanumeric characters, plus "
"these non alphanumeric chars: /.?&+:-@ and spaces are allowed."
msgstr ""
"La entrada contiene caracteres inválidos. Todos los caracteres alfanuméricos "
"más los caracteres /.?'&+:-@ y los espacios están permitidos."

#: src/EBox/CGI/Base.pm:250 src/EBox/CGI/Base.pm:326
msgid ""
"An internal error has ocurred. This is most probably a bug, relevant "
"information can be found in the logs."
msgstr ""
"Se ha producido un error interno. Probablemente se trate de un bug, puede "
"encontrar más información en los logs."

#: src/EBox/CGI/Base.pm:268
msgid ""
"An unknown internal error has ocurred. This is a bug, relevant information "
"can be found in the logs."
msgstr ""
"Se ha producido un error interno. Se trata de un bug, puede encontrar más "
"información en los logs."

#: src/EBox/CGI/Base.pm:288
msgid "You have just hit a bug in eBox. Please seek technical support."
msgstr "Acaba de encontrar un bug en eBox. Por favor, consulte a su soporte técnico."

#: src/EBox/CGI/Base.pm:335
msgid ""
"An internal error related to a template has occurred. This is a bug, "
"relevant information can be found in the logs."
msgstr ""
"Se ha producido un error interno relacionado con una plantilla. Se trata de "
"un bug, puede encontrar más información en los logs."

#: src/EBox/NetWrappers.pm:101 src/EBox/NetWrappers.pm:129
#: src/EBox/NetWrappers.pm:160 src/EBox/NetWrappers.pm:192
msgid "Interface"
msgstr "Interfaz"

#: src/EBox/Config.pm.in:40
msgid "Could not open the config file."
msgstr "No se pudo abrir el fichero de configuración"

#: src/EBox/Config.pm.in:73
msgid "The ebox user has not been set in the config file."
msgstr "El usuario de ebox no ha sido establecido en el fichero de configuración."

#: src/EBox/Sudo.pm:57
#, perl-brace-format
msgid "Command '{cmd}' failed"
msgstr "El comando '{cmd}' falló"

#: src/EBox/Sudo.pm:86
#, perl-brace-format
msgid "Root command '{cmd}' failed"
msgstr "El comando '{cmd}', ejecutado como root, falló"

#: src/EBox/Sudo.pm:111
#, perl-brace-format
msgid "Running command '{cmd}' as {user} failed"
msgstr "El comando '{cmd}', ejecutado como {user}, falló"

#: src/templates/title.mas:12
msgid "Display help notes"
msgstr "Ver notas de ayuda"

#: src/templates/title.mas:13
msgid "show help"
msgstr "mostrar ayuda"

#: src/templates/language.mas:10
msgid "Language selection"
msgstr "Selección de idioma"

#: src/templates/language.mas:12
msgid ""
"Language changes happen automatically, saving the configuration it's not "
"needed."
msgstr ""
"Los cambios de idioma se efectúan de forma automática, no es necesario "
"guardar la configuración"

